{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn sass && remix build",
    "dev": "concurrently \"npm run sass:watch\" \"remix dev\"",
    "start": "remix-serve build",
    "sass": "sass app/:app/",
    "sass:watch": "sass --watch app/:app/",
    "typecheck": "tsc",
    "lint:fix": "eslint 'app/**/*.{ts,tsx}' --fix",
    "lint": "eslint 'app/**/*.{ts,tsx}'",
    "prettify": "prettier --write  \"./src/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "highlight.js": "^11.7.0",
    "isbot": "^3.6.8",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
